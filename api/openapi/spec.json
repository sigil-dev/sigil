  "info": {
    "description": "Secure AI agent gateway API",
    "title": "Sigil Gateway",
    "version": "0.1.0"
  },
  "openapi": "3.1.0",
  "paths": {
    "/api/v1/chat": {
      "post": {
        "operationId": "send-message",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/SendMessageInputBody"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SendMessageOutputBody"
                }
              }
            },
            "description": "OK"
          },
          "403": {
            "content": {
              "application/problem+json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorModel"
                }
              }
            },
            "description": "Forbidden"
          },
          "422": {
            "content": {
              "application/problem+json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorModel"
                }
              }
            },
            "description": "Unprocessable Entity"
          },
          "429": {
            "content": {
              "application/problem+json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorModel"
                }
              }
            },
            "description": "Too Many Requests"
          },
          "500": {
            "content": {
              "application/problem+json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorModel"
                }
              }
            },
            "description": "Internal Server Error"
          }
        },
        "summary": "Send a message to the agent",
        "tags": [
          "chat"
        ]
      }
    },
    "/api/v1/chat/stream": {
      "post": {
        "description": "Send a message and receive a streaming response as server-sent events.",
        "operationId": "chat-stream",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ChatStreamRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "content": {
              "text/event-stream": {
                "schema": {
                  "description": "Each oneOf object in the array represents one possible Server Sent Events (SSE) message.",
                  "items": {
                    "oneOf": [
                      {
                        "properties": {
                          "data": {
                            "description": "The event payload (JSON object)."
                          },
                          "event": {
                            "const": "done",
                            "description": "The event name.",
                            "type": "string"
                          }
                        },
                        "required": [
                          "data",
                          "event"
                        ],
                        "title": "Event done",
                        "type": "object"
                      },
                      {
                        "properties": {
                          "data": {
                            "description": "The event payload (JSON object)."
                          },
                          "event": {
                            "const": "error",
                            "description": "The event name.",
                            "type": "string"
                          }
                        },
                        "required": [
                          "data",
                          "event"
                        ],
                        "title": "Event error",
                        "type": "object"
                      },
                      {
                        "properties": {
                          "data": {
                            "description": "The event payload (JSON object)."
                          },
                          "event": {
                            "const": "session_id",
                            "description": "The event name.",
                            "type": "string"
                          }
                        },
                        "required": [
                          "data",
                          "event"
                        ],
                        "title": "Event session_id",
                        "type": "object"
                      },
                      {
                        "properties": {
                          "data": {
                            "description": "The event payload (JSON object)."
                          },
                          "event": {
                            "const": "text_delta",
                            "description": "The event name.",
                            "type": "string"
                          }
                        },
                        "required": [
                          "data",
                          "event"
                        ],
                        "title": "Event text_delta",
                        "type": "object"
                      },
                      {
                        "properties": {
                          "data": {
                            "description": "The event payload (JSON object)."
                          },
                          "event": {
                            "const": "tool_call",
                            "description": "The event name.",
                            "type": "string"
                          }
                        },
                        "required": [
                          "data",
                          "event"
                        ],
                        "title": "Event tool_call",
                        "type": "object"
                      },
                      {
                        "properties": {
                          "data": {
                            "description": "The event payload (JSON object)."
                          },
                          "event": {
                            "const": "usage",
                            "description": "The event name.",
                            "type": "string"
                          }
                        },
                        "required": [
                          "data",
                          "event"
                        ],
                        "title": "Event usage",
                        "type": "object"
                      }
                    ]
                  },
                  "title": "Server Sent Events",
                  "type": "array"
                }
              }
            },
            "description": "OK"
          },
          "403": {
            "content": {
              "application/problem+json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorModel"
                }
              }
            },
            "description": "Forbidden"
          },
          "422": {
            "content": {
              "application/problem+json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorModel"
                }
              }
            },
            "description": "Unprocessable Entity"
          },
          "429": {
            "content": {
              "application/problem+json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorModel"
                }
              }
            },
            "description": "Too Many Requests"
          },
          "500": {
            "content": {
              "application/problem+json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorModel"
                }
              }
            },
            "description": "Internal Server Error"
          },
          "503": {
            "content": {
              "application/problem+json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorModel"
                }
              }
            },
            "description": "Service Unavailable"
          }
        },
        "summary": "Stream a chat response via SSE",
        "tags": [
          "chat"
        ]
      }
    },
